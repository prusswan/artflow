%article{"data-role" => "page"}
  %header{"data-role" => "header"}
    %h1= @project.name
  %section{"data-role" => "content"}
    %ul{"data-role" => "listview"}
      - @creations.each do |creation|
        %li
          = image_tag creation.file.url(:thumbnail)
          = link_to creation.name, [@project, creation]
